  author = {{de Savigny}, Don and Riley, Ian and Chandramohan, Daniel and Odhiambo, Frank and Nichols, Erin and Notzon, Sam and AbouZahr, Carla and Mitra, Raj and Cobos Mu{\~n}oz, Daniel and Firth, Sonja and Maire, Nicolas and Sankoh, Osman and Bronson, Gay and Setel, Philip and Byass, Peter and Jakob, Robert and Boerma, Ties and Lopez, Alan D.},
  year = {2017},

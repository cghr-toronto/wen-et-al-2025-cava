@article{thomasVerbalAutopsyHealth2018,
  title = {Verbal Autopsy in Health Policy and Systems: A Literature Review},
  shorttitle = {Verbal Autopsy in Health Policy and Systems},
  author = {Thomas, Lisa-Marie and D'Ambruoso, Lucia and Balabanova, Dina},
  year = {2018},
